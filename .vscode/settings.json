{
  "mochaExplorer.files": "test/**/*.ts",
  "mochaExplorer.require": "ts-node/register",
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": true
  },
  "cSpell.language": "en-GB",
  "cSpell.words": [
    "apigatewayv",
    "Chainable",
    "esbuild",
    "eventbridge",
    "exts",
    "hotswap",
    "istanbuljs",
    "Millis",
    "nanoid",
    "outfile",
    "plusplus",
    "resourcegroupstaggingapi",
    "Runtimes",
    "stepfunctions",
    "TRes",
    "vars"
  ],
  "workbench.colorCustomizations": {
    "[Default Dark+]": {
      "titleBar.activeBackground": "#008585",
      "sideBar.background": "#003f3f",
      "activityBar.background": "#008585",
      "activityBar.activeBackground": "#00d4d4"
    }
  },
  "serverlessConsole.services": [
    {
      "type": "cloudformation",
      "title": "Request API Test",
      "timeOffsetInMs": 0,
      "awsProfile": "default",
      "stacks": [
        {
          "region": "eu-west-2",
          "stackName": "RequestApiTestStack",
          "stage": "dev"
        }
      ]
    }
  ]
}
