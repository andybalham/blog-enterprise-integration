{
  "name": "blog-enterprise-integration",
  "version": "1.0.0",
  "description": "Source code for blog post on enterprise integration.",
  "main": "index.js",
  "types": "index.d.ts",
  "scripts": {
    "lint": "eslint ./src --ext .ts",
    "lint-and-fix": "eslint ./src --ext .ts --fix",
    "build": "npm run lint && tsc",
    "test-request-api-synth": "cdk synth RequestApiTestStack --app \"npx ts-node tests/request-api-test-app.ts\" > .cdk-synth.yml",
    "test-request-api-deploy": "cdk deploy RequestApiTestStack --app \"npx ts-node tests/request-api-test-app.ts\"",
    "test-request-api-hotswap": "cdk deploy RequestApiTestStack --hotswap --app \"npx ts-node tests/request-api-test-app.ts\"",
    "test-request-api-destroy": "cdk destroy RequestApiTestStack --app \"npx ts-node tests/request-api-test-app.ts\"",
    "test-quote-processor-synth": "cdk synth QuoteProcessorTestStack --app \"npx ts-node tests/quote-processor-test-app.ts\" > .cdk-synth.yml",
    "test-quote-processor-deploy": "cdk deploy QuoteProcessorTestStack --app \"npx ts-node tests/quote-processor-test-app.ts\"",
    "test-quote-processor-hotswap": "cdk deploy QuoteProcessorTestStack --hotswap --app \"npx ts-node tests/quote-processor-test-app.ts\"",
    "test-quote-processor-destroy": "cdk destroy QuoteProcessorTestStack --app \"npx ts-node tests/quote-processor-test-app.ts\"",
    "test-credit-bureau-synth": "cdk synth CreditBureauTestStack --app \"npx ts-node tests/credit-bureau-test-app.ts\" > .cdk-synth.yml",
    "test-credit-bureau-deploy": "cdk deploy CreditBureauTestStack --app \"npx ts-node tests/credit-bureau-test-app.ts\"",
    "test-credit-bureau-hotswap": "cdk deploy CreditBureauTestStack --hotswap --app \"npx ts-node tests/credit-bureau-test-app.ts\"",
    "test-credit-bureau-destroy": "cdk destroy CreditBureauTestStack --app \"npx ts-node tests/credit-bureau-test-app.ts\"",
    "test-destroy-all": "cdk destroy --all --app \"npx ts-node tests/test-cdk-app.ts\"",
    "cdk-synth": "cdk synth ApplicationStack > .cdk-synth.yml",
    "cdk-deploy": "cdk deploy ApplicationStack",
    "cdk-destroy": "cdk destroy --all"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/andybalham/blog-enterprise-integration.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/andybalham/blog-enterprise-integration/issues"
  },
  "homepage": "https://github.com/andybalham/blog-enterprise-integration#readme",
  "devDependencies": {
    "@types/aws-lambda": "^8.10.81",
    "@types/jest": "^27.4.1",
    "@types/node": "^14.14.31",
    "@typescript-eslint/eslint-plugin": "^4.28.5",
    "@typescript-eslint/parser": "^4.28.5",
    "esbuild": "^0.12.17",
    "eslint": "^7.32.0",
    "eslint-config-airbnb-typescript": "^12.3.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-config-standard": "^16.0.3",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-promise": "^5.1.0",
    "jest": "^27.5.1",
    "prettier": "^2.3.2",
    "source-map-support": "^0.5.19",
    "ts-jest": "^27.1.4",
    "ts-node": "^10.1.0",
    "typescript": "^4.3.5"
  },
  "dependencies": {
    "@andybalham/cdk-cloud-test-kit": "^1.0.8",
    "@andybalham/state-machine-builder-v2": "^2.0.3",
    "aws-cdk-lib": "^2.42.0",
    "aws-sdk": "^2.1218.0",
    "axios": "^0.27.2",
    "constructs": "^10.1.106",
    "nanoid": "^3.3.4",
    "node-fetch": "^2.6.7"
  }
}
