    "jest": "^27.5.1",
    "prettier": "^2.3.2",
    "source-map-support": "^0.5.19",
    "ts-jest": "^27.1.4",
    "ts-node": "^10.1.0",
    "typescript": "^4.3.5"
  },
  "dependencies": {
    "@andybalham/cdk-cloud-test-kit": "^1.0.8",
    "@andybalham/state-machine-builder-v2": "^2.0.3",
    "aws-cdk-lib": "^2.42.0",
    "aws-sdk": "^2.1218.0",
    "axios": "^0.27.2",
    "constructs": "^10.1.106",
    "dotenv": "^16.0.3",
