{
  "name": "blog-enterprise-integration",
  "version": "1.0.0",
  "description": "Source code for blog post on enterprise integration.",
  "main": "index.js",
  "types": "index.d.ts",
  "scripts": {
    "lint": "eslint ./src --ext .ts",
    "lint-and-fix": "eslint ./src --ext .ts --fix",
    "build": "npm run lint && tsc",
    "test-request-api-synth": "cdk synth RequestApiTestStack --app \"npx ts-node tests/request-api-test-app.ts\" > .cdk-synth.yml",
    "test-request-api-deploy": "cdk deploy RequestApiTestStack --app \"npx ts-node tests/request-api-test-app.ts\"",
    "test-request-api-hotswap": "cdk deploy RequestApiTestStack --hotswap --app \"npx ts-node tests/request-api-test-app.ts\"",
    "test-request-api-destroy": "cdk destroy RequestApiTestStack --app \"npx ts-node tests/request-api-test-app.ts\"",
    "test-request-api": "npx jest ./tests/request-api/request-api.test.ts",
    "test-loan-broker-synth": "cdk synth LoanBrokerTestStack --app \"npx ts-node tests/loan-broker-test-app.ts\" > .cdk-synth.yml",
    "test-loan-broker-deploy": "cdk deploy LoanBrokerTestStack --app \"npx ts-node tests/loan-broker-test-app.ts\"",
    "test-loan-broker-hotswap": "cdk deploy LoanBrokerTestStack --hotswap --app \"npx ts-node tests/loan-broker-test-app.ts\"",
    "test-loan-broker-destroy": "cdk destroy LoanBrokerTestStack --app \"npx ts-node tests/loan-broker-test-app.ts\"",
    "test-loan-broker": "npx jest ./tests/loan-broker/loan-broker.test.ts",
    "test-credit-bureau-synth": "cdk synth CreditBureauTestStack --app \"npx ts-node tests/credit-bureau-test-app.ts\" > .cdk-synth.yml",
    "test-credit-bureau-deploy": "cdk deploy CreditBureauTestStack --app \"npx ts-node tests/credit-bureau-test-app.ts\"",
    "test-credit-bureau-hotswap": "cdk deploy CreditBureauTestStack --hotswap --app \"npx ts-node tests/credit-bureau-test-app.ts\"",
    "test-credit-bureau-destroy": "cdk destroy CreditBureauTestStack --app \"npx ts-node tests/credit-bureau-test-app.ts\"",
    "test-credit-bureau": "npx jest ./tests/credit-bureau/credit-bureau.test.ts",
    "test-lender-gateway-synth": "cdk synth LenderGatewayTestStack --app \"npx ts-node tests/lender-gateway-test-app.ts\" > .cdk-synth.yml",
    "test-lender-gateway-deploy": "cdk deploy LenderGatewayTestStack --app \"npx ts-node tests/lender-gateway-test-app.ts\"",
    "test-lender-gateway-hotswap": "cdk deploy LenderGatewayTestStack --hotswap --app \"npx ts-node tests/lender-gateway-test-app.ts\"",
    "test-lender-gateway-destroy": "cdk destroy LenderGatewayTestStack --app \"npx ts-node tests/lender-gateway-test-app.ts\"",
    "test-lender-gateway": "npx jest ./tests/tests/lender-gateway/lender-gateway.test.ts",
    "test-destroy-all": "cdk destroy --all --app \"npx ts-node tests/test-cdk-app.ts\"",
    "loan-broker-synth": "cdk synth LoanBrokerStack > .cdk-synth.yml",
    "loan-broker-deploy": "cdk deploy LoanBrokerStack",
    "loan-broker-destroy": "cdk destroy LoanBrokerStack",
    "request-api-synth": "cdk synth RequestApiStack > .cdk-synth.yml",
    "request-api-deploy": "cdk deploy RequestApiStack",
    "request-api-destroy": "cdk destroy RequestApiStack",
    "credit-bureau-synth": "cdk synth CreditBureauStack > .cdk-synth.yml",
    "credit-bureau-deploy": "cdk deploy CreditBureauStack",
    "credit-bureau-destroy": "cdk destroy CreditBureauStack",
    "webhook-synth": "cdk synth WebhookStack > .cdk-synth.yml",
    "webhook-deploy": "cdk deploy WebhookStack",
    "webhook-destroy": "cdk destroy WebhookStack",
    "application-deploy": "cdk deploy LoanBrokerStack RequestApiStack CreditBureauStack WebhookStack",
    "lenders-deploy": "cdk deploy LenderSteadyStack Lender666Stack LenderSmallBundlesStack",
    "destroy-all": "cdk destroy --all"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/andybalham/blog-enterprise-integration.git"
